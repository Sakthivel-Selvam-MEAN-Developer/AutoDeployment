{
    "name": "backend",
    "private": true,
    "version": "0.0.0",
    "devDependencies": {
        "@types/compression": "^1.7.3",
        "@types/cors": "^2.8.14",
        "@types/express": "^4.17.18",
        "@types/jest": "^29.5.5",
        "@types/morgan": "^1.9.6",
        "@types/node": "^20.8.3",
        "@typescript-eslint/eslint-plugin": "^6.9.1",
        "@typescript-eslint/parser": "^6.9.1",
        "eslint": "^8.51.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-plugin-import": "^2.25.2",
        "eslint-plugin-jest": "^27.4.0",
        "prettier": "3.0.3",
        "prisma": "^5.4.2",
        "ts-jest": "^29.1.1",
        "ts-node": "^10.9.1",
        "typescript": "^5.2.2",
        "vite": "^4.4.5",
        "vite-plugin-node": "^3.0.2",
        "vitest": "^0.34.6"
    },
    "dependencies": {
        "@jest/globals": "^29.7.0",
        "@ory/cli": "^0.2.2",
        "@ory/client": "^1.3.0",
        "@prisma/client": "5.5.2",
        "axios": "^1.5.1",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "dayjs": "^1.11.10",
        "dotenv-cli": "^7.3.0",
        "express": "^4.18.2",
        "helmet": "^7.0.0",
        "morgan": "^1.10.0",
        "nock": "^13.3.4",
        "winston": "^3.11.0"
    },
    "scripts": {
        "dev": "vite",
        "proxy": "ory proxy --no-jwt --port 4000 http://localhost:3000/",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "lint": "eslint ./src --max-warnings 157",
        "prismaGenMove": "prisma migrate dev --schema=./src/wonderMove/prisma/schema.prisma --create-only --name ",
        "prismaGenCheckMove": "prisma migrate status --schema=./src/wonderMove/prisma/schema.prisma ",
        "prismaUpMove": "prisma generate --schema=./src/wonderMove/prisma/schema.prisma",
        "prismaMigrateDevMove": "prisma migrate deploy --schema=./src/wonderMove/prisma/schema.prisma",
        "prismaMigrateTestMove": "dotenv -e .env.test -- prisma migrate deploy --schema=./src/wonderMove/prisma/schema.prisma",
        "migrateCIDb": "dotenv -e .env.ci -- prisma migrate deploy",
        "test": "dotenv -e .env.test -- jest --watch --runInBand",
        "t": "dotenv -e .env.test -- jest --watch",
        "format": "prettier -c src",
        "prismaMove": "yarn prismaUpMove; yarn prismaMigrateDevMove; yarn prismaMigrateTestMove",
        "prismaHrm": "prisma generate --schema=./src/hrm/prisma/schema.prisma",
        "prismaHrmDev": "prisma migrate dev --schema=./src/hrm/prisma/schema.prisma --create-only --name",
        "prismaHrmDeploy": "prisma migrate deploy --schema=./src/hrm/prisma/schema.prisma",
        "prismaHrmTest": "dotenv -e .env.test -- prisma migrate deploy --schema=./src/hrm/prisma/schema.prisma"
    }
}
