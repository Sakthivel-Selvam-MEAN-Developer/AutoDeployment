{
    "name": "backend",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "devDependencies": {
        "@eslint/js": "^9.6.0",
        "@rollup/plugin-node-resolve": "^15.2.3",
        "@types/compression": "^1.7.5",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/jsdom": "^21.1.7",
        "@types/morgan": "^1.9.9",
        "@types/node": "^20.10.5",
        "@types/supertest": "^6.0.2",
        "@vitest/coverage-v8": "^1.6.0",
        "eslint": "^9.6.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-plugin-import": "^2.29.1",
        "globals": "^15.8.0",
        "prettier": "3.3.2",
        "prisma": "^5.16.1",
        "react-dom-server": "^0.0.5",
        "typescript": "^5.5.3",
        "typescript-eslint": "^7.15.0",
        "vite": "^5.3.3",
        "vite-plugin-node": "^3.1.0",
        "vitest": "^1.6.0"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.606.0",
        "@prisma/client": "5.16.1",
        "@types/html-pdf": "^3.0.3",
        "@types/jsonwebtoken": "^9.0.6",
        "@types/pdfkit": "^0.13.4",
        "aws-sdk": "^2.1650.0",
        "axios": "^1.7.2",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "dayjs": "^1.11.11",
        "dotenv-cli": "^7.3.0",
        "express": "^4.19.2",
        "helmet": "^7.1.0",
        "html-pdf": "^3.0.1",
        "html2canvas": "^1.4.1",
        "jsdom": "^24.1.0",
        "jsonwebtoken": "^9.0.2",
        "jspdf": "^2.5.1",
        "keycloak-connect": "^24.0.1",
        "morgan": "^1.10.0",
        "nock": "^13.5.4",
        "pdf-lib": "^1.17.1",
        "pdfkit": "^0.15.0",
        "pm2": "^5.3.0",
        "puppeteer": "^22.12.1",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-dom-server": "^0.0.5",
        "supertest": "^7.0.0",
        "to-words": "^4.0.1",
        "winston": "^3.11.0"
    },
    "scripts": {
        "dev": "vite --host",
        "build": "tsc && vite build",
        "runProd": "pm2-runtime start ecosystem.config.cjs --env production",
        "preview": "vite preview",
        "lint": "eslint ./src --report-unused-disable-directives --max-warnings 456",
        "pris": "yarn prismaUp; yarn prismaMigrateDev; yarn prismaMigrateTest",
        "prismaGen": "prisma migrate dev --create-only --name ",
        "prismaGenCheck": "prisma migrate status ",
        "prismaUp": "prisma generate",
        "prismaMigrateDev": "prisma migrate deploy",
        "prismaMigrateTest": "dotenv -e .env.test -- prisma migrate deploy",
        "prismaMigrateCi": "dotenv -e .env.ci -- prisma migrate deploy",
        "test": "dotenv -e .env.test -- vitest --coverage.enabled=true",
        "testCI": "dotenv -e .env.ci -- vitest  --coverage.enabled=true",
        "testOnce": "dotenv -e .env.test -- vitest --run --coverage.enabled=true --coverage.thresholds.autoUpdate=true",
        "t": "dotenv -e .env.test -- vitest --poolOptions.threads.singleThread=true --coverage.enabled=false",
        "format": "prettier -c src",
        "seedDevDb": "vite-node --script src/devSeed/index.ts"
    }
}
